{
  "name": "jzero",
  "displayName": "Jzero SyntaxColorer",
  "description": "Jzero programming language",
  "author": "Matthew MacFarquhar",
  "license": "public domain",
  "version": "0.1.0",
  "publisher": "mm",
  "engines": {
